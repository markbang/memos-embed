{
	"name": "@memos-embed/react",
	"version": "0.1.0",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/markbang/memos-embed.git"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsup src/index.tsx --format esm,cjs --dts --clean",
		"test": "vitest run"
	},
	"peerDependencies": {
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0"
	},
	"dependencies": {
		"memos-embed": "workspace:*"
	}
}
